<?xml version='1.0' encoding='utf-8'?>
<widget id="com.infinity.hmspluginsdemo" version="5.2.0.302" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HMSMap">
        <param name="android-package" value="com.huawei.hms.cordova.map.HMSMap" />
    </feature>
    <name>hmspluginsdemo</name>
    <description>
        A sample application that demonstrates HMS Maps Plugin features on Cordova.
    </description>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
